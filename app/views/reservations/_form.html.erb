<div class="form-Inshow">
  <%= simple_form_for [experience, reservation] do |f| %>
  <p>Book your experience !</p>
  <%= f.input :participants_number  %>
  <%= f.input :start_at, as: :string, required: false, input_html: {id: "start_at"}, class: "datepicker" %>
  <%= f.input :end_date, as: :string, required: false, input_html: {id: "end_date"}, class: "datepicker" %>
  <%= f.button :submit , class: "btn btn-green"%>
  <% end %>
</div>

